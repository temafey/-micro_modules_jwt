{
    "name": "micro-module/jwt",
    "type": "library",
    "description": "Micro module jwt common library",
    "license": "proprietary",
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "php-parallel-lint/php-console-highlighter": "^0.4",
        "php-parallel-lint/php-parallel-lint": "^1.0",
        "mockery/mockery": "^1.3",
        "phpmd/phpmd": "^2.8",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-mockery": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-master",
        "symplify/easy-coding-standard": "^7.2",
        "vimeo/psalm": "^4.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MicroModule\\JWT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MicroModule\\JWT\\Tests\\Unit\\": "tests/unit"
        }
    }
}
